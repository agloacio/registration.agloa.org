{
	"name": "post-and-page-builder-premium",
	"version": "1.2.3",
	"description": "Post and Page Builder is a standalone plugin which adds functionality to the existing TinyMCE Editor.",
	"main": "dist/editor.min.js",
	"scripts": {
		"dev": "npm run dev-webpack && gulp build",
		"dev-webpack": "webpack build --mode=development --progress=profile",
		"start": "webpack-dev-server",
		"build-webpack": "cross-env NODE_ENV=production webpack",
		"format-js": "prettier-eslint \"src/assets/**/*.js\" --write",
		"format-sass": "stylelint \"assets/**/*.scss\" --fix --syntax scss",
		"build": "npm --trace-deprecation run build-webpack"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BoldGrid/post-and-page-builder-premium.git"
	},
	"keywords": [
		"Editor",
		"tinyMCE"
	],
	"author": "BoldGrid",
	"license": "GPL-2.0",
	"bugs": {
		"url": "https://github.com/BoldGrid/post-and-page-builder/issues"
	},
	"homepage": "https://github.com/BoldGrid/post-and-page-builder#readme",
	"devDependencies": {
		"@babel/core": "7.21.4",
		"@babel/eslint-parser": "^7.21.3",
		"@babel/plugin-proposal-export-default-from": "^7.18.0",
		"@babel/preset-env": "^7.21.4",
		"@wordpress/eslint-plugin": "^14.5.0",
		"babel-loader": "^9.1.2",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^4.5.2",
		"css-loader": "^6.7.3",
		"eslint": "^7.32.0",
		"eslint-plugin-html": "^4.0.5",
		"file-loader": "^2.0.0",
		"html-loader": "^0.5.5",
		"husky": "^0.14.3",
		"lint-staged": "^7.2.2",
		"mini-css-extract-plugin": "^2.7.5",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"postcss": "^8.4.23",
		"postcss-loader": "^7.2.4",
		"prettier-eslint": "^8.8.2",
		"prettier-eslint-cli": "^4.7.1",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"slick-carousel": "https://github.com/boldgrid/slick#v1.8.3",
		"style-loader": "^0.23.0",
		"stylelint": "^9.5.0",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-webpack-plugin": "^0.10.5",
		"svg-inline-loader": "^0.8.0",
		"terser-webpack-plugin": "^2.2.1",
		"url-loader": "^1.1.1",
		"webpack": "^5.81.0",
		"webpack-cli": "^5.0.2",
		"webpack-dev-server": "^4.13.3"
	},
	"dependencies": {
		"@babel/preset-react": "^7.22.3",
		"@boldgrid/controls": "https://github.com/BoldGrid/controls#carousel",
		"@wordpress/components": "^25.0.0",
		"@wordpress/element": "^5.11.0",
		"@wordpress/escape-html": "^2.55.0",
		"@wordpress/html-entities": "^3.57.0",
		"boldgrid-panel": "^1.2.3",
		"cross-env": "^6.0.3",
		"html2canvas": "^1.4.1",
		"tinycolor2": "^1.6.0",
		"styled-components": "^6.0.0-rc.3"
	}
}
